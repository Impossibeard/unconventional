<%= form_for [@project, @post] do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
<br>
  <%= f.label :description %>
  <%= f.text_area :description %>
<br>
  <%= f.hidden_field :picture_cache %>
  <%= f.file_field :picture %>

  <%= f.submit "Post Content" %>
<% end %>

